{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "apkTest": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "node": "20.19.4"
    },
    "apkRelease": {
      "distribution": "store",
      "android": {
        "buildType": "apk"
      },
      "node": "20.19.4"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "node": "20.19.4"
    }
  },
  "submit": {
    "production": {}
  }
}
